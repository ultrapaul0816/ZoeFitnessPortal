# Copy this file to .env and fill in your actual values
# For Replit: Add these to Secrets tab (ðŸ”’) instead

# App Configuration
APP_URL=https://app.strongerwithzoe.com
NODE_ENV=production

# Database (Neon PostgreSQL)
DATABASE_URL=postgresql://user:password@host/database

# Session
SESSION_SECRET=your-random-secret-here-change-this

# Email (Resend)
RESEND_API_KEY=re_your_resend_api_key_here
RESEND_FROM_EMAIL=Stronger With Zoe <noreply@strongerwithzoe.in>

# AI Features - Required for coach remarks and plan generation
# Set ANTHROPIC_API_KEY to use Claude (preferred), or OPENAI_API_KEY for OpenAI fallback
# If both are set, Claude is used
ANTHROPIC_API_KEY=sk-ant-your-anthropic-api-key-here
OPENAI_API_KEY=sk-your-openai-api-key-here

# Image Uploads (Optional - Cloudinary)
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret

# Spotify Integration (Optional)
SPOTIFY_CLIENT_ID=your_spotify_client_id
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret
SPOTIFY_REDIRECT_URI=https://app.strongerwithzoe.com/api/spotify/callback

# Shopify Webhook (Optional)
SHOPIFY_WEBHOOK_SECRET=your_shopify_webhook_secret
